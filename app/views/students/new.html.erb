<h1>Student Form</h1>

<%= form_tag students_path do %>
  <label>Student Form</label><br>
  <%= text_field_tag :'students[first_name]' %><br>

  <label>Post description:</label><br>
  <%= text_area_tag :'students[last_name]' %><br>

  <%= submit_tag "Submit Post" %>
<% end %>

<%= params.inspect %>
